<Window x:Class="BeASwitch.IntroWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeASwitch"
        mc:Ignorable="d"
        Title="IntroWindow" Height="180" Width="126">
    <Grid>
        <CheckBox x:Name="checkBoxUseVLAN" Content="Use VLAN" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonStart" Content="Start" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top" Width="95" Click="buttonStart_Click"/>

    </Grid>
</Window>
